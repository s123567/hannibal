
<div class="modal fade mobile-all-user-likes" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content list-quotes">
      <br>
      <% current_user.upvotes.all.each do |u| %>
      <li class="text-center mobile-modal-quote"><%= u.quote.content %></li>
      <li class="mobile-modal-info text-center"><p><%= link_to '<i class="fa fa-heart" aria-hidden="true"></i>'.html_safe, upvote_path(u), class:'likes', method: :delete %> <%= link_to u.quote.user.name, user_path(u.quote.user_id) %> </li>
      <hr class='hr-red'>
      <% end %>
      
    </div>
  </div>
</div>
